{
  "source": {
    "excludePattern": "\\.preprocess\\.js$",
    "exclude": [ "node_modules", "docs", "dist", "test", "temp", "coverage" ],
    "include": [ ".", "README.md" ]
  },

  "opts": {
    "destination": "docs",
    "recurse": true,
    "private": false
  },

  "plugins": [
    "plugins/markdown"
  ]
}
